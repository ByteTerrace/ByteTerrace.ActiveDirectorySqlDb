<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>main</TargetDatabaseName>
    <DeployScriptFileName>ByteTerrace.ActiveDirectorySqlDb.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=byteterrace.database.windows.net;Persist Security Info=False;User ID=kittoes@byteterrace.com;Pooling=False;Multiple Active Result Sets=False;Connect Timeout=60;Encrypt=True;Trust Server Certificate=False;Authentication=ActiveDirectoryInteractive;Command Timeout=0</TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <DatabaseEdition>Premium</DatabaseEdition>
    <DatabaseServiceObjective>P1</DatabaseServiceObjective>
    <DatabaseMaximumSize>100</DatabaseMaximumSize>
    <ScriptDatabaseCollation>True</ScriptDatabaseCollation>
    <ScriptDatabaseCompatibility>True</ScriptDatabaseCompatibility>
    <CreateNewDatabase>False</CreateNewDatabase>
    <DeployDatabaseInSingleUserMode>True</DeployDatabaseInSingleUserMode>
    <ScriptDatabaseOptions>False</ScriptDatabaseOptions>
  </PropertyGroup>
</Project>